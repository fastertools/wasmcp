{
  "name": "wasmcp-wasi",
  "version": "0.1.0",
  "type": "module",
  "description": "WASI SDK for building MCP (Model Context Protocol) WebAssembly components in TypeScript",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "import": "./dist/http.js"
    },
    "./keyvalue": {
      "types": "./dist/keyvalue.d.ts",
      "import": "./dist/keyvalue.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    }
  },
  "files": [
    "dist",
    "types",
    "wit"
  ],
  "scripts": {
    "build": "tsc && cp -r types/* dist/",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "wasmcp": "^0.1.6"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "config": {
    "witDependencies": [
      {
        "witPath": "./wit",
        "package": "wasmcp:wasi@0.1.0",
        "world": "mcp-wasi"
      }
    ]
  }
}