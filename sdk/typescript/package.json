{
  "name": "wasmcp",
  "version": "0.1.13",
  "type": "module",
  "description": "TypeScript SDK for building MCP (Model Context Protocol) WebAssembly components",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
